config {
    absRefPrefix = /
    linkVars = L
    sys_language_uid = 0
    sys_language_overlay = 1
    sys_language_mode = content_fallback
    language = en
    locale_all = en_US.UTF-8
}

# Dutch
[globalVar = GP:L = 1]
    config {
        sys_language_uid = 1
        language = nl
        locale_all = nl_NL.UTF-8
    }
[global]

# German
[globalVar = GP:L = 2]
    config {
        sys_language_uid = 2
        language = de
        locale_all = de_DE.UTF-8
    }
[global]
